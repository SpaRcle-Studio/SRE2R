ShaderType Simple;

PolygonMode Fill;
CullMode Back;
DepthCompare LessOrEqual;
PrimitiveTopology TriangleList;
BlendEnabled false;
DepthWrite true;
DepthTest true;

void fragment() {
	COLOR = vec4(FRAG_COORD.z, 0.0, 0.0, 1.0);
}

void vertex() {
    OUT_POSITION = PROJECTION_MATRIX * VIEW_MATRIX * MODEL_MATRIX * vec4(VERTEX, 1.0);
}